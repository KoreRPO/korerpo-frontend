{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "resolveJsonModule": true,
    "rootDirs": ["./app", "./public/assets"],
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNEXT"
  },
  "exclude": ["./index.ts"],
  "include": [
    "./node_modules/@dryan-llc/mnml.js/dist/**/*.d.ts",
    "./**/*.ts",
    "./**/*.tsx"
  ]
}
